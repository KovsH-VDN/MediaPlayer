<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="LAccentColor">#FFFFBBFF</Color>
    <Color x:Key="NAccentColor">#FFFF88FF</Color>
    <Color x:Key="HAccentColor">#FFFF66FF</Color>

    <SolidColorBrush x:Key="LAccentBrush" Color="{DynamicResource LAccentColor}"/>
    <SolidColorBrush x:Key="NAccentBrush" Color="{DynamicResource NAccentColor}"/>
    <SolidColorBrush x:Key="HAccentBrush" Color="{DynamicResource HAccentColor}"/>
</ResourceDictionary>